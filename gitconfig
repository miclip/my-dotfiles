[alias]
  st=status
  di=diff
  dc=diff --cached
  co=checkout
  ci=duet-commit
  br=branch
  sta=stash
  llog="log --date=local"
  flog="log --pretty=fuller --decorate"
  lol="log --graph --decorate --oneline"
  lola="log --graph --decorate --oneline --all"
  blog="log origin/master... --left-right"
  ds="diff --staged"
  fixup="commit --fixup"
  squash="commit --squash"
  unstage="reset HEAD"
  rum="rebase master@{u}"
  sur="submodule update --init --recursive"
